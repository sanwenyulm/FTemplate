<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="my" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="false" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection connectionURL="jdbc:mysql://localhost:3306/iotyun?useSSL=false"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator targetPackage="com.iotyun.manager.entity"
			targetProject="manager/src/main/java">

			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.iotyun.manager.mapper"
			targetProject="manager/src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.iotyun.manager.mapper"
			targetProject="manager/src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<table tableName="alart" domainObjectName="Alart">
		</table>
		<table tableName="alart_status" domainObjectName="AlartRecord">
		</table>
		<table tableName="auth" domainObjectName="UserAuth">
		</table>
		<table tableName="device" domainObjectName="Device">
		</table>
		<table tableName="sensor" domainObjectName="Sensor">
		</table>
		<table tableName="monitor" domainObjectName="Monitor">
		</table>
				<table tableName="user" domainObjectName="User">
		</table>
				<table tableName="user_pwd" domainObjectName="UserLogin">
		</table>
	</context>
</generatorConfiguration>